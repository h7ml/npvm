{
  "name": "npvm",
  "version": "0.2.0",
  "description": "Node Package Manager Visual Platform - A modern web UI for managing npm/yarn/pnpm/bun packages",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h7ml/npvm.git"
  },
  "homepage": "https://github.com/h7ml/npvm#readme",
  "bugs": {
    "url": "https://github.com/h7ml/npvm/issues"
  },
  "author": {
    "name": "h7ml",
    "url": "https://github.com/h7ml"
  },
  "contributors": [
    {
      "name": "h7ml",
