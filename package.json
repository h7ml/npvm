    }
  ],
  "license": "MIT",
  "keywords": [
    "npm",
    "yarn",
    "pnpm",
    "bun",
    "package-manager",
    "visual",
    "gui",
    "node",
    "nodejs",
    "dependency",
    "security",
    "audit",
    "registry",
    "monorepo"
  ],
  "scripts": {
